{
  "name": "@augensturn/element",
  "version": "1.0.0",
  "description": "Vue3 component library",
  "type": "module",
  "author": "augensturn",
  "license": "MIT",
  "keywords": [
    "component",
    "UI",
    "Vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Augensturn/my-AugUI-npm.git"
  },
  "homepage": "https://github.com/Augensturn/my-AugUI-npm",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/umd/au-element.umd.cjs",
  "module": "./dist/es/au-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/au-element.js",
      "require": "./dist/umd/au-element.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
     "build-library": "npm run build-only && npm run move-style",
    "build-demo": "vite build --outDir demo-dist",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style":  "node -e \"const fs = require('fs'); fs.copyFileSync('dist/es/index.css', 'dist/index.css')\"",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitepress-demo-preview/component": "^2.5.1",
    "@vitepress-demo-preview/plugin": "^1.4.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "move-file": "^3.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "postcss-for": "^2.1.1",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-scss": "^4.0.9",
    "postcss-simple-vars": "^7.0.1",
    "sass": "^1.90.0",
    "typescript": "~5.8.0",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.4"
  }
}
